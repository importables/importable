{
	"$schema": "https://json.schemastore.org/package",
	"name": "@importable/yaml",
	"description": "Import YAML files with strong typing based on import attributes",
	"type": "module",
	"version": "0.1.0",
	"exports": {
		".": {
			"import": "./lib/importable-yaml-tsconfig.mjs",
			"require": "./lib/importable-yaml-tsconfig.cjs"
		},
		"./astro": {
			"import": "./lib/importable-yaml-astro.mjs"
		},
		"./next": {
			"import": "./lib/importable-yaml-next.mjs",
			"require": "./lib/importable-yaml-next.cjs"
		},
		"./tsconfig": {
			"import": "./lib/importable-yaml-tsconfig.mjs",
			"require": "./lib/importable-yaml-tsconfig.cjs"
		},
		"./vite": {
			"import": "./lib/importable-yaml-rollup.mjs"
		},
		"./webpack": {
			"require": "./lib/importable-yaml-webpack.mjs"
		}
	},
	"files": [
		"lib"
	],
	"dependencies": {
		"yaml": "^2.2"
	},
	"devDependencies": {
		"astro": "^2.4",
		"importable": "*",
		"next": "^13.4",
		"vite": "^4.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/importables/importable.git",
		"directory": "packages/importable-yaml"
	},
	"homepage": "https://github.com/importables/importable/",
	"bugs": "https://github.com/importables/importable/issues",
	"author": "importables",
	"license": "CC0-1.0",
	"keywords": [
		"assert",
		"asserts",
		"assertions",
		"attribute",
		"attributes",
		"file",
		"files",
		"import",
		"importable",
		"importable-plugin",
		"imports",
		"plugin",
		"javascript",
		"js",
		"jsconfig",
		"rollup",
		"rollup-plugin",
		"strong",
		"ts",
		"tsconfig",
		"type",
		"typed",
		"types",
		"typescript",
		"typescript-plugin",
		"typing",
		"vite",
		"vite-plugin",
		"yaml"
	]
}
